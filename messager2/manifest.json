{
    "manifest_version": 2,
    "name": "Messager2",
    "description": "new one",
    "version": "1.1.3",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "contentScript.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "http://*/*",
        "https://*/*",
        "tabs",
        "webNavigation"
    ]
}